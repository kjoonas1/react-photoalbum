{
  "name": "photoalbum",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.15"
  },
  "engines": {
    "npm": "6.11.3",
    "node": "10.17.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-pre-commit": "react-scripts test --env=jsdom --watchAll=false",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
    "lint-fix": "./node_modules/.bin/eslint . --ext .js --ext .jsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix && npm run test-pre-commit",
      "pre-push": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.1.6",
    "eslint-plugin-react": "^7.15.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "react-test-renderer": "^16.9.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  }
}